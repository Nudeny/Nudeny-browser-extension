{
    "manifest_version": 3,
    "name" : "Nudeny",
    "description" : "Nudity image detection and classification",
    "version" : "1.0",
    "permissions": ["tabs"],
    "host_permissions": [
                    "http://127.0.0.1:8000/*"],
    "action":{
        "default_popup" : "popup.html"
    },
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "all_frames": true,
          "js": ["contentScript.js"]
        }
      ]
}